{
  "schemaVersion": 1,
  "id": "audiblecamera",
  "version": "0.2.0",
  "name": "Audible Camera",
  "description": "An accessibility mod that provides an audible camera system for exploring the world. Press C to activate, then use arrow keys to navigate and hear block names. Alt+Up/Down for vertical movement. Designed for blind and visually impaired players.",
  "authors": ["nzingilou"],
  "contact": {
    "homepage": "https://github.com/nzingilou/audiblecamera",
    "sources": "https://github.com/nzingilou/audiblecamera",
    "issues": "https://github.com/nzingilou/audiblecamera/issues"
  },
  "license": "MIT",
  "icon": "assets/audiblecamera/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.audiblecamera.AudibleCameraMod"
    ]
  },
  "mixins": [
    "audiblecamera.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": "~1.21.8",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "cloth-config": "*"
  }
} 